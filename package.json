    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-confirm-alert": "^2.7.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-image-crop": "^9.0.3",
    "react-loading": "^2.0.3",
    "react-lottie-player": "^1.4.0",
    "react-moment": "^1.1.1",
